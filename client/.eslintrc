{
  "extends": ["../.eslintrc", "plugin:react/recommended"],

  "env": {
    "browser": true
  },

  "plugins": [
    "react"
  ],

  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },

  "rules": {
    "react/jsx-uses-react": 2,
    "react/prop-types": 0,
    "react/react-in-jsx-scope": 2
  }
}
